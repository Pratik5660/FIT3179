{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Percentage of Children who repoted having been bullied in the last term (2018)",
  "width": 800,
  "height": 600,
  "projection": {"rotate":[219,60,0],"type":"azimuthalEqualArea"},
  "layer": [
    {
    "data": {
      "url": "https://raw.githubusercontent.com/cywng/FIT3179/main/VICLGAtopojson.json",
      "format": {"type": "topojson", "feature": "VICLGAgeojson"}},
      "transform": [
        {
          "lookup": "properties.vic_lga__3",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/cywng/FIT3179/main/Bullying2018Vic.csv"
              },
              "key": "LGA",
              "fields": ["Percent"]
          }
        }
      ], 
      "mark": {"type": "geoshape"},
      "encoding":{
        "color": {
          "field":"Percent",
          "type":"quantitative"
          },
          "tooltip": [
            {"field": "properties.vic_lga__3", "type": "nominal", "title": "Local Government Area"},
            {"field": "Percent", "type": "quantitative"}
          ]

      }
    }
    ]
}
