{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Proportion of Year 7-9 students who reported being bullied in the last term (2016)",
  "width": "container",
  "height": 350,
  "projection": {
    "type": "conicEqualArea",
    "parallels": [
      -36,
      -38
    ],
    "rotate": [
      -145,
      0,
      0
    ]
  },
  "data": {
    "url": "https://raw.githubusercontent.com/cywng/FIT3179/main/Data/VICLGAtopojson.json",
    "format": {
      "type": "topojson",
      "feature": "VICLGAgeojson"
    }
  },
  "encoding": {
    "tooltip": [
      {
        "field": "properties.vic_lga__3",
        "type": "nominal",
        "title": "Local Government Area"
      },
      {
        "field": "Portion Bullied",
        "type": "quantitative",
        "title": "Percent Bullied"
      }
    ]
  },
  "layer": [
    {
      "mark": {
        "type": "geoshape",
        "fill": "gainsboro",
        "stroke": "white"
      }
    },
    {
      "transform": [
        {
          "lookup": "properties.vic_lga__3",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/cywng/FIT3179/main/Data/JoinedDataFiltered.csv"
            },
            "key": "LGA",
            "fields": [
              "Portion Bullied"
            ]
          }
        }
      ],
      "mark": {
        "type": "geoshape"
      },
      "encoding": {
        "color": {
          "field": "Portion Bullied",
          "type": "quantitative",
          "title": [
            "Proportion of",
            "students bullied"
          ],
          "scale": {
            "domain": [
              0,
              35
            ]
          }
        }
      }
    }
  ]
}